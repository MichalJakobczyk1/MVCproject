@model Employee

<div class="container">
    <h1 class="my-4">
        Page Heading
        <small>Secondary Text</small>
    </h1>
    <div class="row">
        <div class="col-md-4">
            <h3 class="my-3">@Model.Name @Model.Surname</h3>
            <h4>
            <ul>
                <li>@Model.DateOfBirth</li>
                <li>@Model.HiredOn</li>
            </ul>
            </h4>
            <h4>
            <ul>
                <li>@Model.Email</li>
                <li>@Model.ContactNumber</li>
            </ul>
            </h4>
        </div>

    </div>
</div>
